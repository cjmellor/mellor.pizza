{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.5",
        "hammerstone/fast-paginate": "^0.1.10",
        "larabug/larabug": "^2.5.3",
        "laravel/fortify": "^1.13.7",
        "laravel/framework": "^9.39",
        "laravel/sanctum": "2.14.1",
        "laravel/tinker": "^2.7.2",
        "league/flysystem-aws-s3-v3": "^3.10",
        "livewire/livewire": "^2.10.7",
        "lukeraymonddowning/honey": "^0.4.0",
        "spatie/laravel-disable-floc": "^1.0.2",
        "torchlight/torchlight-commonmark": "^0.5.5"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.20",
        "laravel/pint": "^0.2.4",
        "laravel/sail": "^1.16.2",
        "mockery/mockery": "^1.5.1",
        "nunomaduro/collision": "^6.3.1",
        "nunomaduro/larastan": "^0.7.15",
        "nunomaduro/patrol": "^1.1",
        "phpunit/phpunit": "^9.5.26",
        "spatie/laravel-ignition": "^1.6.1",
        "spatie/laravel-ray": "^1.31"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
